{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars":"warn",
        "semi":[2,"always"],
        "indent":["warn",4],
        "eqeqeq":"warn",
        "brace-style":[2,"stroustrup"]
    }
}
